Nopoly" selector="textCssUntenAn:">textCssUntenAn: index	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	^index =  (2 * kantenlaenge + 1) | (index = (3 * kantenlaenge +1)) ifTrue: ['eck_text'] ifFalse: ['text_senkrecht']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>css</category><body package="NoNopoly" selector="textCssObenAn:">textCssObenAn: index	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	^index = 1 | (index = (kantenlaenge +1)) ifTrue: ['eck_text'] ifFalse: ['text_senkrecht']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>css</category><body package="NoNopoly" selector="definiereSpielbrettCss:">definiereSpielbrettCss: spielfeldGroesse "px"	"hier werden dynamisch CSS-Klassen für die Felder auf dem Spielbrett bestimmt. Die Größen werden dabei 	prozentual zur spielfeldGroesse ermittelt."	| kantenlaenge csseckfeld cssfeldsenkrecht cssfeldwaagerecht cssspalte cssmittelfeld spielfeld rastermass|	kantenlaenge := self model spielbrettGroesse // 4.	rastermass := (spielfeldGroesse  / (kantenlaenge + 3) asFloat ) truncated.	csseckfeld := '.ecke{   width: ' , (2 * rastermass)  printString  , 'px;   height: ' , (2 * rastermass) printString , 'px;  float:left;} '.	cssmittelfeld := '.mitte{   width: ' , (kantenlaenge -1 * rastermass) truncated printString, 'px;   height: ' , (kantenlaenge - 1 * rastermass )  truncated printString,  'px;  float:left;}'.	cssfeldsenkrecht := '.senkrecht{   width: ' , rastermass printString, 'px;   height: ' , (2 * rastermass) printString, 'px;   float:left;}'.	cssfeldwaagerecht := '.waagerecht{   height:', (1 / (kantenlaenge -1) * 100.0)  printString, '%;   width:100%;}'.	cssspalte := '.spalte{   height: ' , (kantenlaenge - 1 * rastermass) printString, 'px;   width: ' , (2 * rastermass) printString , 'px;   float:left;}'.       spielfeld := '#spielfeld{  margin: auto;  width: ', spielfeldGroesse printString, 'px;  height: ' ,spielfeldGroesse printString, 'px;}'.	NoNopolyFileLibrary default felderCss: csseckfeld, cssfeldwaagerecht, cssfeldsenkrecht, cssmittelfeld, cssspalte, spielfeld</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>css</category><body package="NoNopoly" selector="aussenCssUntenAn:">aussenCssUntenAn: index	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	^index = (2 * kantenlaenge + 1) | (index = (3* kantenlaenge +1)) ifTrue: ['ecke'] ifFalse: ['senkrecht']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>css</category><body package="NoNopoly" selector="aussenCssObenAn:">aussenCssObenAn: index	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	^index = 1 | (index = (kantenlaenge +1)) ifTrue: ['ecke'] ifFalse: ['senkrecht']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>css</category><body package="NoNopoly" selector="innenCssUntenAn:">innenCssUntenAn: index	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	^index = (2 * kantenlaenge + 1) | (index = (3* kantenlaenge +1)) ifTrue: ['eckfeld'] ifFalse: ['feld']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="NoNopoly" selector="renderKopfzeileOn:">renderKopfzeileOn: html	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	1 to: kantenlaenge + 1		do: 			[:i |			(html div)				class: (self model feldAnPosition: i) class name asLowercase;				class: (self aussenCssObenAn: i);				with: 						[(html div)							class: (self innenCssObenAn: i);							with: [html paragraph class: (self textCssObenAn: i); with: (self model feldnameAnPosition: i)]]]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="NoNopoly" selector="renderContentOn:">renderContentOn: html	self renderKopfdatenOn: html.	self model anzahlTeilnehmer = 0		ifTrue: [self renderSpielerEingabeOn: html]		ifFalse: [self renderSpielerAnzeigeOn: html].	self renderSpielbrettOn: html</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="NoNopoly" selector="renderSpielerAnzeigeOn:">renderSpielerAnzeigeOn: html	| lfdNr |	html		paragraph: 'Es gibt jetzt ' , self model anzahlTeilnehmer printString , ' Teilnehmer: '.	lfdNr := 0.	self model teilnehmer do: 			[:tlnhmr |			lfdNr := lfdNr + 1.			(html div)				class: 'spieler' , lfdNr printString;				with: 						[html							text: 'Name: ' , tlnhmr name;							break;							text: 'Position: ' , tlnhmr position printString;							break;							text: 'Guthaben: ' , tlnhmr guthaben printString;							break]]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="NoNopoly" selector="renderMittelfeldOn:">renderMittelfeldOn: html	(html div)		class: 'mitte';		with: [html div class: 'mittelfeld'; with: 'Monopoly']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="NoNopoly" selector="renderLinkeSpalteOn:">renderLinkeSpalteOn: html	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	(html div)		class: 'spalte';		with: 				[self model spielbrettGroesse to: self model spielbrettGroesse - (kantenlaenge - 2)					by: -1					do: 						[:i |						(html div)							class: (self model feldAnPosition: i) class name asLowercase;							class: 'waagerecht';							with: 									[(html div)										class: 'text';										class: 'feld';										with:  [html paragraph: (self model feldnameAnPosition: i)]]]]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="NoNopoly" selector="renderKopfdatenOn:">renderKopfdatenOn: html	html heading with: 'Hier ist NoNopoly'.	html paragraph: 			[html text: '(Autor: Hans Täuscher, Firma: '.			(html anchor)				url: 'http://www.nordakademie.de';				with: 'NORDAKADEMIE '.			html text: ')']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="NoNopoly" selector="renderSpielbrettOn:">renderSpielbrettOn: html	(html heading)		level: 2;		class: 'clear';		with: 'Spielbrett'.	(html div)		id: 'spielfeld';		with: 				[self renderKopfzeileOn: html.				html div class: 'clear'.				self renderLinkeSpalteOn: html.				self renderMittelfeldOn: html.				self renderRechteSpalteOn: html.				html div class: 'clear'.				self renderFusszeileOn: html]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="NoNopoly" selector="renderFusszeileOn:">renderFusszeileOn: html	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	3 * kantenlaenge +1  to: 2* kantenlaenge +1 by: -1		do: 			[:i |			(html div)				class: (self model feldAnPosition: i) class name asLowercase;				class: (self aussenCssUntenAn: i);				with: 						[(html div)							class: (self innenCssUntenAn: i);							with: [html paragraph class: (self textCssUntenAn: i); with: (self model feldnameAnPosition: i)]]]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="NoNopoly" selector="renderSpielerEingabeOn:">renderSpielerEingabeOn: html	| spielerNamen |	html form: 			[html paragraph: 					[html text: 'Anzahl der Spieler: '.					(html select)						list: #(1 2 3 4 5 6);						callback: [:anzahl | self spielerEingabe anzahlFelder: anzahl].					(html submitButton)						callback: 								[spielerNamen := self call: self spielerEingabe.								self model registriereTeilnehmerMitNamen: spielerNamen];						text: 'Bitte Spielernamen eingeben']]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="NoNopoly" selector="renderRechteSpalteOn:">renderRechteSpalteOn: html	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	(html div)		class: 'spalte';		with: 				[kantenlaenge + 2 to: 2 * kantenlaenge					do: 						[:i |						(html div)							class: (self model feldAnPosition: i) class name asLowercase;							class: 'waagerecht';							with: 									[(html div)										class: 'text';										class: 'feld';										with: [html paragraph: (self model feldnameAnPosition: i)]]]]</body></methods><methods><class-id>NoNopolyNs.NoNopolyFileLibrary</class-id> <category>accessing</category><body package="NoNopoly" selector="felderCss:">felderCss: anObject	felderCss := anObject</body></methods><methods><class-id>NoNopolyNs.NoNopolyFileLibrary</class-id> <category>accessing</category><body package="NoNopoly" selector="felderCss">felderCss	^felderCss</body></methods><methods><class-id>NoNopolyNs.NoNopolyFileLibrary</class-id> <category>uploaded</category><body package="NoNopoly" selector="nonopolynebeneinanderCss">nonopolynebeneinanderCss | css |css :=' .spieler1	{display:block; width:200px; background-color:#339900; float: left;}			 .spieler2	{display:block; width:200px; background-color:#0066cc;float: left;}							 .spieler3	{display:block; width:200px; background-color:#ff0000;float: left;}			 .spieler4	{display:block; width:200px; background-color:#ff9900;float: left;}							 .spieler5	{display:block; width:200px; background-color:#ffff00;float: left;}			 .spieler6	{display:block; width:200px; background-color:#000000; color:#ffffff;float: left;}	.strassefabrik 	{background-color:#339900;}	.losfabrik 		{background-color:#0066cc;}.bahnhoffabrik 	{background-color:#ff0000; }.werkfabrik 		{background-color:#ffff00;}.polizistfabrik 	{background-color:#ff9900;}.freiparkenfabrik 	{background-color:#cccc00;}.gefaengnisfabrik {background-color:#cc6600;}.spielbrett {position: relative;} body{  height:100%;  width:100%;  font-family: sans-serif;  font-size: 16px;}.text{    padding: 5px;    text-align: center;}.eck_text{    padding: 45% 10px ;   text-align: center;}.text_senkrecht{   -moz-transform: rotate(90deg);   -ms-transform: rotate(90deg);   -o-transform: rotate(90deg);   -webkit-transform: rotate(90deg);      transform:rotate(90deg);   padding:20%;   white-space:nowrap;}#spielfeld{  margin: auto;  width: 600px;  height: 600px;}.eckfeld{   width: 100%;   height: 100%;   border: 1px solid white;}.feld{   height:100%;   width:100%;   border: 1px solid white;}.mittelfeld{   width: 100%;   height: 100%;   text-align:center;   float:left;   border: 1px solid white;   background-color: lightgreen;}.clear{   clear:both;}'.css := css , self felderCss.^ css</body></methods><methods><class-id>NoNopolyNs.SpielerEingabe</class-id> <category>accessing</category><body package="NoNopoly" selector="anzahlFelder:">anzahlFelder: anObject	anzahlFelder := anObject</body></methods><methods><class-id>NoNopolyNs.SpielerEingabe</class-id> <category>accessing</category><body package="NoNopoly" selector="fehlermeldung">fehlermeldung	^fehlermeldung</body></methods><methods><class-id>NoNopolyNs.SpielerEingabe</class-id> <category>accessing</category><body package="NoNopoly" selector="nameAn:">nameAn: anInteger	^self namen size &gt;= anInteger		ifTrue: [self namen at: anInteger]		ifFalse: [self namen add: '']</body></methods><methods><class-id>NoNopolyNs.SpielerEingabe</class-id> <category>accessing</category><body package="NoNopoly" selector="anzahlFelder">anzahlFelder	^anzahlFelder</body></methods><methods><class-id>NoNopolyNs.SpielerEingabe</class-id> <category>accessing</category><body package="NoNopoly" selector="fehlermeldung:">fehlermeldung: anObject	fehlermeldung := anObject</