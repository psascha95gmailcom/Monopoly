ntual zur spielfeldGroesse ermittelt."	| kantenlaenge csseckfeld cssfeldsenkrecht cssfeldwaagerecht cssspalte cssmittelfeld spielfeld rastermass|	kantenlaenge := self model spielbrettGroesse // 4.	rastermass := (spielfeldGroesse  / (kantenlaenge + 3) asFloat ) truncated.	csseckfeld := '.ecke{   width: ' , (2 * rastermass)  printString  , 'px;   height: ' , (2 * rastermass) printString , 'px;  float:left;} '.	cssmittelfeld := '.mitte{   width: ' , (kantenlaenge -1 * rastermass) truncated printString, 'px;   height: ' , (kantenlaenge - 1 * rastermass )  truncated printString,  'px;  float:left;}'.	cssfeldsenkrecht := '.senkrecht{   width: ' , rastermass printString, 'px;   height: ' , (2 * rastermass) printString, 'px;   float:left;}'.	cssfeldwaagerecht := '.waagerecht{   height:', (1 / (kantenlaenge -1) * 100.0)  printString, '%;   width:100%;}'.	cssspalte := '.spalte{   height: ' , (kantenlaenge - 1 * rastermass) printString, 'px;   width: ' , (2 * rastermass) printString , 'px;   float:left;}'.       spielfeld := '#spielfeld{  margin: auto;  width: ', spielfeldGroesse printString, 'px;  height: ' ,spielfeldGroesse printString, 'px;}'.	NoNopolyFileLibrary default felderCss: csseckfeld, cssfeldwaagerecht, cssfeldsenkrecht, cssmittelfeld, cssspalte, spielfeld</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>css</category><body package="Non-Commercialization" selector="aussenCssUntenAn:">aussenCssUntenAn: index	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	^index = (2 * kantenlaenge + 1) | (index = (3* kantenlaenge +1)) ifTrue: ['ecke'] ifFalse: ['senkrecht']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>css</category><body package="Non-Commercialization" selector="aussenCssObenAn:">aussenCssObenAn: index	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	^index = 1 | (index = (kantenlaenge +1)) ifTrue: ['ecke'] ifFalse: ['senkrecht']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>css</category><body package="Non-Commercialization" selector="innenCssUntenAn:">innenCssUntenAn: index	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	^index = (2 * kantenlaenge + 1) | (index = (3* kantenlaenge +1)) ifTrue: ['eckfeld'] ifFalse: ['feld']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="Non-Commercialization" selector="renderKopfzeileOn:">renderKopfzeileOn: html	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	1 to: kantenlaenge + 1		do: 			[:i |			(html div)				class: (self model feldAnPosition: i) class name asLowercase;				class: (self aussenCssObenAn: i);				with: 						[(html div)							class: (self innenCssObenAn: i);							with: [html paragraph class: (self textCssObenAn: i); with: (self model feldnameAnPosition: i)]]]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="Non-Commercialization" selector="renderContentOn:">renderContentOn: html	self renderKopfdatenOn: html.	self model anzahlTeilnehmer = 0		ifTrue: [self renderSpielerEingabeOn: html]		ifFalse: [self renderSpielerAnzeigeOn: html].	self renderSpielbrettOn: html</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="Non-Commercialization" selector="renderSpielerAnzeigeOn:">renderSpielerAnzeigeOn: html	| lfdNr |	html		paragraph: 'Es gibt jetzt ' , self model anzahlTeilnehmer printString , ' Teilnehmer: '.	lfdNr := 0.	self model teilnehmer do: 			[:tlnhmr |			lfdNr := lfdNr + 1.			(html div)				class: 'spieler' , lfdNr printString;				with: 						[html							text: 'Name: ' , tlnhmr name;							break;							text: 'Position: ' , tlnhmr position printString;							break;							text: 'Guthaben: ' , tlnhmr guthaben printString;							break]]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="Non-Commercialization" selector="renderMittelfeldOn:">renderMittelfeldOn: html	(html div)		class: 'mitte';		with: [html div class: 'mittelfeld'; with: 'Monopoly']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="Non-Commercialization" selector="renderLinkeSpalteOn:">renderLinkeSpalteOn: html	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	(html div)		class: 'spalte';		with: 				[self model spielbrettGroesse to: self model spielbrettGroesse - (kantenlaenge - 2)					by: -1					do: 						[:i |						(html div)							class: (self model feldAnPosition: i) class name asLowercase;							class: 'waagerecht';							with: 									[(html div)										class: 'text';										class: 'feld';										with:  [html paragraph: (self model feldnameAnPosition: i)]]]]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="Non-Commercialization" selector="renderKopfdatenOn:">renderKopfdatenOn: html	html heading with: 'Hier ist NoNopoly'.	html paragraph: 			[html text: '(Autor: Hans Täuscher, Firma: '.			(html anchor)				url: 'http://www.nordakademie.de';				with: 'NORDAKADEMIE '.			html text: ')']</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="Non-Commercialization" selector="renderSpielbrettOn:">renderSpielbrettOn: html	(html heading)		level: 2;		class: 'clear';		with: 'Spielbrett'.	(html div)		id: 'spielfeld';		with: 				[self renderKopfzeileOn: html.				html div class: 'clear'.				self renderLinkeSpalteOn: html.				self renderMittelfeldOn: html.				self renderRechteSpalteOn: html.				html div class: 'clear'.				self renderFusszeileOn: html]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="Non-Commercialization" selector="renderFusszeileOn:">renderFusszeileOn: html	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	3 * kantenlaenge +1  to: 2* kantenlaenge +1 by: -1		do: 			[:i |			(html div)				class: (self model feldAnPosition: i) class name asLowercase;				class: (self aussenCssUntenAn: i);				with: 						[(html div)							class: (self innenCssUntenAn: i);							with: [html paragraph class: (self textCssUntenAn: i); with: (self model feldnameAnPosition: i)]]]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="Non-Commercialization" selector="renderSpielerEingabeOn:">renderSpielerEingabeOn: html	| spielerNamen |	html form: 			[html paragraph: 					[html text: 'Anzahl der Spieler: '.					(html select)						list: #(1 2 3 4 5 6);						callback: [:anzahl | self spielerEingabe anzahlFelder: anzahl].					(html submitButton)						callback: 								[spielerNamen := self call: self spielerEingabe.								self model registriereTeilnehmerMitNamen: spielerNamen];						text: 'Bitte Spielernamen eingeben']]</body></methods><methods><class-id>NoNopolyNs.NoNopolyMain</class-id> <category>rendering</category><body package="Non-Commercialization" selector="renderRechteSpalteOn:">renderRechteSpalteOn: html	| kantenlaenge |	kantenlaenge := self model spielbrettGroesse // 4.	(html div)		class: 'spalte';		with: 				[kantenlaenge + 2 to: 2 * kantenlaenge					do: 						[:i |						(html div)							class: (self model feldAnPosition: i) class name asLowercase;							class: 'waagerecht';							with: 									[(html div)										class: 'text';										class: 'feld';										with: [html paragraph: (self model feldnameAnPosition: i)]]]]</body></methods><methods><class-id>NoNopolyNs.NoNopolyFileLibrary</class-id> <category>accessing</