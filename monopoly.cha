 selectors |			selectors := badMessages inject: Set new						into: 							[:set :each | 							set								addAll: (context selectedClass whichSelectorsReferTo: each);								yourself].			selectors 				do: [:each | result addClass: context selectedClass selector: each].			selectors notEmpty ifTrue: [result searchStrings: badMessages]].	^detector</body><body package="Browser-Smalllint" selector="longMethods">longMethods	| detector matcher |	detector := self new.	detector name: (#LongMethods &lt;&lt; #browser &gt;&gt; 'Long methods') asString.	matcher := ParseTreeSearcher new.	matcher matches: '`.Stmt'		do: 			[:aNode :answer | 			(aNode children inject: answer				into: [:sum :each | matcher executeTree: each initialAnswer: sum]) + 1].	detector methodBlock: 			[:context :result | 			(matcher executeTree: context parseTree initialAnswer: 0) 				&gt;= self longMethodSize 					ifTrue: [result addClass: context selectedClass selector: context selector]].	^detector</body><body package="Browser-Smalllint" selector="utilityMethods">utilityMethods	| detector |	detector := self new.	detector name: (#UtilityMethods &lt;&lt; #browser &gt;&gt; 'Utility methods') asString.	detector methodBlock: 			[:context :result | 			(context selectedClass isMeta | (context selector numArgs == 0) or: 					[context protocols anySatisfy: 							[:each | 							self utilityProtocols anySatisfy: [:protocol | protocol match: each]]]) 				ifFalse: 					[(self subclassOf: context selectedClass overrides: context selector) 						ifFalse: 							[(context superMessages isEmpty and: [context selfMessages isEmpty]) 								ifTrue: 									[(context parseTree isPrimitive or: 											[(context selectedClass allInstVarNames 												, context selectedClass allClassVarNames asArray) , #('self') 												anySatisfy: [:each | context parseTree references: each]]) 										ifFalse: [result addClass: context selectedClass selector: context selector]]]]].	^detector</body></methods><methods><class-id>Seaside.WAComponent class</class-id> <category>*seaside-tools-core</category><body package="Seaside-Tools-Core" selector="registerAsApplication:">registerAsApplication: aString 	self 		greaseDeprecatedApi: 'WAComponent class&gt;&gt;#registerAsApplication:'		details: 'Use WAAdmin class&gt;&gt;#register:asApplicationAt:.'.	^ WAAdmin 		register: self		asApplicationAt: aString</body></methods><methods><class-id>Prog1Start.WebTranscript class</class-id> <category>class initialization</category><body package="Prog1Start" selector="defineGlobalVariable">defineGlobalVariableSmalltalk.Core defineSharedVariable: #WTranscript	private: false	constant: false	category: 'Interface-Text'	initializer: nil</body><body package="Prog1Start" selector="initialize">initialize	"self initialize"	Seaside.WAAdmin register: self asApplicationAt: 'Web-Transcript'.	WText := WriteStream on: String new.	self defineGlobalVariable.	WTranscript := WebTranscript</body></methods><methods><class-id>Prog1Start.WebTranscript class</class-id> <category>accessing</category><body package="Prog1Start" selector="clear">clear	WText := WriteStream on: String new.	self flush</body><body package="Prog1Start" selector="cr">cr	WText cr</body><body package="Prog1Start" selector="flush">flush	SiouX.SeasideServer start.	ExternalWebBrowser		open: 'http://localhost:7777/Web-Transcript'</body><body package="Prog1Start" selector="show:">show: aByteString	WText nextPutAll: aByteString.	self flush</body><body package="Prog1Start" selector="space">space	WText space</body><body package="Prog1Start" selector="tab">tab	WText tab</body></methods><class><name>ObjectFigure</name><environment>Refactory.HotDraw</environment><super>Refactory.HotDraw.CachedFigure</super><private>false</private><indexed-type>none</indexed-type><inst-vars>object inEdges outEdges </inst-vars><class-inst-vars></class-inst-vars><imports></impo